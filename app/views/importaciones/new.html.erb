<h1><%= t 'titulo.importar' %></h1>

<br/>

<%= form_tag('/importaciones', :multipart => true, 'accept-charset' => "ISO-8859-1") do %>
	<fieldset style="width: 500px">
		<legend><%= t 'titulo.importar' %></legend>
		<div class="field">
			<%= file_field_tag 'archivo' %>
		</div>
		<div class="field">
			<span class="tip"><%= t 'mensajes.tip_carga_archivo_import' %></span>
		</div>
		<div class="actions">
			<%= submit_tag(t('procesar'), :class => 'inputButton') %>
		</div>  
	</fieldset>
<% end %>

<h1><%=t 'titulo.lugares' %></h1>

<table>
  <tr>
    <th><%= t :nombre %></th>
    <th><%= t :direccion %></th>
    <th><%= t :url %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lugares.each do |lugar| %>
  <tr>
    <td><%= lugar.nombre %></td>
    <td><%= lugar.direccion %></td>
    <td><%= lugar.url %></td>
    <td><%= link_to 'Show', lugar %></td>
    <td><%= link_to 'Edit', edit_lugar_path(lugar) %></td>
    <td><%= link_to 'Destroy', lugar, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Lugar', new_lugar_path %>
